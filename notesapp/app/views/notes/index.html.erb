<div class="container">
    <div class="row">

        <div class="col-lg-12">
            <h1 class="page-header">My Notes <span style="font-size: 12px;">Add a note</span></h1>
        </div>

        <% @notes.each do |note| %>
            <div class="col-lg-3 col-md-4 col-xs-6 thumb">
                <a class="thumbnail" href="#">
                    <h2><%= note.title %></h2>
                    <%= note.note %>
                    <%= note.updated_at %>
                    <%= note.created_at %>
                </a>
            </div>
        <% end %>
    </div>
</div>